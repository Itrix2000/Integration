;------------------------------------------------
entry lpFindObject
;------------------------------------------------
params
	String pNodeId : IN
endparams

	clear/e "OBJECT"
	PATH.OBJECT = "&uEQ;%%pNodeId%%%"
	retrieve/e "OBJECT"

	return 0

end ;lpFindObject


;------------------------------------------------
entry lpGetFileHash
;------------------------------------------------
returns String

params
	String  pPath  : IN
	String  pRev   : IN
	Boolean pThumb : IN
endparams

variables
	String  vSource
	Raw     vRawHash
	String  vMD5Hash
	Numeric vPos
	String  vFileExtension
	String  vFileName
endvariables

	vSource = $concat(pPath, pRev)

	if(pThumb)
		vSource = $concat(vSource, "thumb")
	endif

	vRawHash = $encode("MD5", vSource)
	vMD5Hash = $encode("HEX", vRawHash)

	vPos = $rscan(pPath, ".")
	vFileExtension = pPath[vPos]

	vFileName = $concat(".\cache\", vMD5Hash, vFileExtension)

	return vFileName

end ;lpGetFileHash
