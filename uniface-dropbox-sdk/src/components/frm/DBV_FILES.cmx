<DAT name="SFUNC" xml:space='preserve'>;----------------------------------------------------
operation openFile
;----------------------------------------------------
params
	String pNodeId : IN
endparams

variables
	String  vType
	Boolean vIsFolder
	String  vFileName
endvariables

	$instancehandle-&gt;getType(pNodeId, vType)
	$instancehandle-&gt;isFolder(vType, vIsFolder)

	if(vIsFolder)
		;This is a folder so don't do anything
		return 0
	endif

	;This is a file so go get it. First, work out where we're
	;going to put it.
	call lpFindObject(pNodeId)
	vFileName = lpGetFileHash(PATH.OBJECT, REV.OBJECT, 0)

	;TODO: Check if we already have this file cached locally

	;Get the file from Dropbox
	$dropboxClient$-&gt;getFile(PATH.OBJECT, vFileName, "", "")

	;Now have Windows open the file
	spawn vFileName

	return 0

end ;openFile


;----------------------------------------------------
operation uploadFile
;----------------------------------------------------
params
	String pNodeId : IN
endparams

variables
	String  vType
	Boolean vIsFolder
	Numeric vPos
	String  vFilePath
	String  vFileName
	String  vPath
	Struct  vOptions
	Struct  vReturn
endvariables

	$instancehandle-&gt;getType(pNodeId, vType)
	$instancehandle-&gt;isFolder(vType, vIsFolder)

	if(!vIsFolder)
		;This isn't a folder, so we can't upload into it
		return 0
	endif

	;Ask the user to select a file for upload
	filebox

	if($status != 1)
		;User didn't select a file so stop here
		return 0
	endif

	;User selected file, grab the path
	vFilePath = $result

	;The path argument to Dropbox should include the file name
	vPos = $rscan(vFilePath, "\")
	vFileName = vFilePath[vPos + 1] 
	vPath = $concat(pNodeId, "/", vFileName)

	vOptions = $newstruct
	vOptions-&gt;"autorename"         = "true"
	vOptions-&gt;"callback_instance"  = $instancename
	vOptions-&gt;"callback_operation" = "updateProgress"

	$dropboxClient$-&gt;uploadFileChunked(vPath, vFilePath, vOptions, vReturn)

	message "Upload of %%vFilePath%%% completed successfully"

	return 0

end ;uploadFile


;----------------------------------------------------
operation getRevisionHistory
;----------------------------------------------------
params
	String pNodeId : IN
endparams

variables
	Struct  vReturn
endvariables

	$dropboxClient$-&gt;getRevisions(pNodeId, "", vReturn)

	REVISIONS.DUMMY = vReturn-&gt;$dbgstringplain

	return 0

end ;getRevisionHistory


;----------------------------------------------------
operation updateProgress
;----------------------------------------------------
params
	Numeric vProcessed : IN
	Numeric vTotal     : IN
endparams

variables
	Numeric vPercentage
endvariables

	vPercentage = (vProcessed / vTotal) * 100
	vPercentage = vPercentage[trunc]

	message "Uploaded %%vProcessed%%% bytes of %%vTotal%%% (%%vPercentage%%%%)"
	show

	return 0

end ;updateProgress


;----------------------------------------------------
operation createFolder
;----------------------------------------------------
params
	String pNodeId : IN
endparams

variables
	String vNewFolderPath
	Struct vMetaData
endvariables

	vNewFolderPath = $concat(pNodeId, "/NewFolder")

	$dropboxClient$-&gt;createFolder(vNewFolderPath, vMetaData)

	;TODO: Improve storeMetadata to handle this. Probably need to pass in a parent ID as well
	;activate "DBV_HELPER_S".storeMetadata(vMetaData)

	call lpExpandNode(pNodeId)
	;call TREE_setObjectExpand(pNodeId, "-")
	;call TREE_flush()


	return 0

end ;createFolder


;----------------------------------------------------
operation isFolder
;----------------------------------------------------
params
	String  pType     : IN
	Boolean pIsFolder : OUT
endparams

	pIsFolder = 0

	if(pType[1:6] == "FOLDER")
		;This is a folder
		pIsFolder = 1
	endif

	return 0

end ;isFolder


;----------------------------------------------------
operation getType
;----------------------------------------------------
params
	String pNodeId : IN
	String pType   : OUT
endparams

	pType = TREE_getObjectType(pNodeId)
	return 0

end ;getType</DAT>
