<DAT name="GENERAL" xml:space='preserve'>;------------------------------------------------
entry lpExpandNode
;------------------------------------------------
params
	String	pID	: IN
endparams

	;Log that this node is open
	putitem/id $NodeState$, pID, "OPEN"

	call lpPopulateNodes(pID)

	return 0

end ;lpExpandNode


;------------------------------------------------
entry lpCollapseNode
;------------------------------------------------
params
	String	pID	: IN
endparams

	;Log that this node is closed
	delitem/id $NodeState$, pID

	return 0

end ;lpCollapseNode


;------------------------------------------------
entry lpPopulateNodes
;------------------------------------------------
params
	String pParentId : IN
endparams

variables
	String vTmp
	Struct pOptions
	Struct vReturn
	String vName
	String vType
	String vParent
	String vExpandState
endvariables

	getitem/id vTmp, $NodesProcessed$, pParentId
	if($status &gt; 0)
		;This node already has its children populated
		return 0
	endif

	;See if we've fetched this folder from Dropbox already
	pOptions = $newstruct

	clear/e "OBJECT"
	PATH.OBJECT/init = "&uEQ;%%pParentId%%%"
	retrieve/e "OBJECT"
	if($status &gt;= 0 &amp; HASH.OBJECT != "")
		pOptions-&gt;"hash" = HASH.OBJECT
	endif

	pOptions-&gt;"include_media_info" = "true"
	pOptions-&gt;"include_membership" = "true"

	$dropboxClient$-&gt;getMetadata(pParentId, pOptions, vReturn)

	activate "DBV_HELPER_S".storeMetadata(vReturn)

	;------------------------------------------------------
	; Get the children of this node and add them in
	;------------------------------------------------------
	clear/e "OBJECT"
	PARENT_PATH.OBJECT/init = "&uEQ;%%pParentId%%%"
	retrieve/e "OBJECT"

	;----------------------------------------------------
	; Add a tree node for each occurrence of OBJECT
	;----------------------------------------------------
	setocc "OBJECT", 1
	while($status &gt; 0)

		vName        = lpGetName(PATH.OBJECT, PARENT_PATH.OBJECT)
		vType        = lpGetType(ICON.OBJECT)
		vExpandState = lpGetExpandState(IS_DIR.OBJECT)

		vParent = PARENT_PATH.OBJECT
		;if(vParent == "/")
		;	vParent = ""
		;endif

		call TREE_setObject(PATH.OBJECT, vParent, vExpandState, vType, vName, "")

		setocc "OBJECT", $curocc("OBJECT") + 1

	endwhile

	call TREE_flush()
	putitem/id $NodesProcessed$, pParentId, "1"

	return 0

end ;lpPopulateNodes


;------------------------------------------------
entry lpGetName
;------------------------------------------------
returns String

params
	String pPath       : IN
	String pParentPath : IN
endparams

variables
	String vName
endvariables

	vName = $ltrim(pPath, $syntax(pParentPath, "CI"))
	vName = $ltrim(vName, "/")

	return vName

end ;lpGetName


;------------------------------------------------
entry lpGetType
;------------------------------------------------
returns String

params
	String pIconName : IN
endparams

variables
	String vType
endvariables

	vType = $uppercase(pIconName)

	return vType

end ;lpGetType


;------------------------------------------------
entry lpGetExpandState
;------------------------------------------------
returns String

params
	String pIsDir : IN
endparams

variables
	String vExpandState
endvariables

	if(pIsDir == "T")
		vExpandState = "+"
	else
		vExpandState = ""
	endif

	return vExpandState

end ;lpGetExandState


