	endif
	return (1)
done

operation frontv
params
	string input_string 	: IN
	boolean frontv		: OUT
endparams
	frontv = "F"
	if (input_string != "")
		scan $vsvcn$, input_string
		if ($result != 1)
			$r3$ = ($result * 2) - 1
		else
			$r3$ = $result
		endif
		activate "METAPHONE".BINARY( $number($vsvfn$[$r3$:2]), $r2$)
		if ($r2$[4:1] = "1") frontv = "T"
	endif
	return (1)
done

operation noghf
params
	string input_string	: IN
	boolean noghf		: OUT
endparams

	noghf = "F"
	scan $vsvcn$, input_string
	if ($result != 1)
		$r3$ = ($result * 2) - 1
	else
		$r3$ = $result
	endif
	activate "METAPHONE".BINARY( $number($vsvfn$[$r3$:2]), $r2$)
	if ($r2$[5:1] = "1") noghf = "T"
	return (1)
done

operation binary
params
	numeric binary_input	: INOUT
	string binary_output	: OUT
endparams

        binary_output = ""
        $r3$ = 0
        $r4$ = 0
        repeat
                $R3$ = (binary_input / 2)
                $R4$ = binary_input - $R3$[trunc] * 2
                binary_input = (binary_input - $R4$) / 2
                binary_output = "%%binary_output%%$r4$"
        until ( binary_input = 0)
	return (1)
done







