 &uFRM;TYP=E&uSEP;NAM=DUMMY.MODEL_1&uSEP;WID=32&uSEP;HEI=5&uSEP;HOC=32&uSEP;VOC=5&uFRM;</DAT>
<DAT name="TITLE" xml:space='preserve'>Metaphone Processing</DAT>
<DAT name="WINPROP">SPLIT=</DAT>
<DAT name="SFUNC" xml:space='preserve'>operation metaphone
params
	string input_string	: IN
	string output_string	: INOUT
endparams


;*------------Metaphone Transformations----------------------------------
;* PN- GN- KN-   N
;* AE-           E
;* WR-           R
;* WH-           H
;* X-            S
;*
;* B             B (unless in -MB)
;* C             X if in -CIA-, -CH-
;* 			else S if in -CI-, -CE-, -CY-
;* 			else dropped if in -SCI-, -SCE-, -SCY-
;* 			else K
;* D             J if in -DGE-, -DGI-, -DGY-
;* 			else T
;* G             F if in -GH and not B--GH, D--GH,-H--GH, -H---GH
;* 			else dropped if -GNED, -GN, -DGE-, -DGI-, -DGY-
;* 			else J if in -GE-, -GI-, -GY- and not GG
;* 			else K
;* H             H if before a vowel and not after C, G, P, S, T
;* 			else dropped
;* K             dropped if after C, else K
;* P             F if before H, else P
;* Q             K
;* S             X in -SIO- or -SIA-, else S
;* T             X in -TIA- or -TIO-
;* 			else O before H
;* 			else T
;* V             F
;* W             W after a vowel, else dropped
;* X             KS
;* Y             Y unless followed by a vowel
;* Z             S
;*
;* Note: F, J, L, M, N, R are never transformed.
;*
;*----------------------------------------------------------------------*/

$vsvfn$ = "0116041609020416090200020202010400020404010000000800"
$vsvcn$ = "ABCDEFGHIJKLMNOPQRSTUVWXYZ"

	activate "METAPHONE".UPCASES( input_string, $workstring$ )
	length $workstring$ 
	if ($result &lt;= 0) done		; Exit if 0 bytes


	$n_end$ = $result       ; set n_end to end of string 
;
	$Cnt$ = 1		; Set pointer to 1
	$WS1$ = ""		; Clear work string
	repeat
;		check for PN KN GN AE WR WH and X at start
		$nptr$ = $workstring$[$cnt$:1]
		$WS2$ = $nptr$
		$R2$ = $cnt$ + 1
		$next$ = $workstring$[$R2$:1]

		if ($WS2$ != "0" ) 
			if ($cnt$ &gt; 1 )
				$WS1$ = "%%$WS1$%%$WS2$"
			else
				$WS1$ = "%%$WS2$"
			endif
		endif
		if ($nptr$ = "P" | $nptr$ = "K" | $nptr$ = "G" )
			if ($next$ = "N") $WS2$ = "0"	
		endif
		if ($nptr$ = "A" &amp; $next$ = "E") $WS2$ = "0"
		if ($nptr$ = "W" )
			if ($next$ = "R" )
				$WS2$ = "0"
			else
				if ($next$ = "H") $WS2$ = "0"
			endif
		endif
		if ($next$ = "X") $WS2$ = "S"
		$cnt$ = $cnt$ + 1
	until ($cnt$ &gt; $n_end$)

	$workstring$ = $WS1$	; Rotate String
	
	$WS1$ = ""		; Clear
	$WS2$ = ""		; Clear
	output_string = ""	; Clear
	length $workstring$
	$n_end$ = $result       ; set n_end to end of string 
;
;	
;	 Now, loop step through string, stopping at end of string
;	 or when the computed 'metaph' is 4 characters long
;	
	$metaph$ = ""	; Set Output to NULL
	$KSflag$ = 0	; State flag for KS translation
	$cnt$ = 1	; Set Counter
	$nptr$ = ""	; Clear String pointer 
	
	while ($cnt$ &lt;= $n_end$)
		length $metaph$
		if ($result = 4) goto alldone
		if ($KSflag$) 
			$KSflag$ = 0	
			output_string = $metaph$
		else 
			$nptr$ = $workstring$[$cnt$:1]	; One character
			$WS1$ = $cnt$ - 1
			if ($cnt$ &gt; 1)
				$previous$ = $workstring$[$WS1$:1]
			else
				$previous$ = ""
			endif
			$WS1$ = $cnt$ + 1
			$next$ = $workstring$[$WS1$:1]
			$WS1$ = $WS1$ + 1
			$plus2$ = $workstring$[$WS1$:1]
			if ($nptr$ = $next$ &amp; $nptr$ != "C")
				goto duploop
			endif
;	 Check for F J L M N R or first letter vowel 
			activate "METAPHONE".same( $nptr$, $same$ )
			activate "METAPHONE".vowel( $nptr$, $vowel$ )
			if ( $same$ | ($cnt$ = 1 &amp; $vowel$) )
				$Metaph$ = "%%$metaph$%%$nptr$"
			else 
				if ($nptr$ = "B")
					if ($cnt$ &lt; $n_end$ | $previous$ != "M")
						$metaph$ = "%%$metaph$%%$nptr$" 
					endif
				endif
	if ($nptr$ = "C")
		activate "METAPHONE".frontv( $next$, $frontv$ )
		if ($previous$ != "S" | !$frontv$)
		  if ($next$ = "I" &amp; $plus2$ = "A")
			$metaph$ = "%%$metaph$X"
		  else
			activate "METAPHONE".frontv( $next$, $frontv$ ) 
			if ($frontv$)
				$metaph$ = "%%$metaph$S"
			else
			  if ($next$ = "H")
				activate "METAPHONE".vowel( $plus2$, $vowel$)
				if ( ($cnt$ = 1 &amp; !$vowel$) | $previous$ = "S")
				   $metaph$ = "%%$metaph$K"
				else
				   $metaph$ = "%%$metaph$X"
				endif
			  else
				$metaph$ = "%%$metaph$K"
		 	  endif
			endif
	  	  endif
		endif
	endif

				if ($nptr$ = "D")
					activate "METAPHONE".frontv( $plus2$, $frontv$ )
					if ($next$ = "G" &amp; $frontv$)
						$metaph$ = "%%$metaph$J"
					else
						$metaph$ = "%%$metaph$T"
					endif
				endif

if ($nptr$ = "G")
	activate "METAPHONE".vowel( $plus2$, $vowel$ )
	activate "METAPHONE".frontv( $next$, $frontv$ )
	$R3$ = ($next$ != "H" | $vowel$)
	$r5$ = ($next$ != "N" | ($cnt$ + 1 &lt; $n_end$))
	$R2$ = $cnt$ + 3
	$R6$ = ($plus2$ != "E" | $workstring$[$R2$:1] != "D")
	$R7$ = ($previous$ != "D" | !$frontv$ )
	if ( $R3$ &amp; $R5$ &amp; $R6$ &amp; $R7$ )
		activate "METAPHONE".frontv( $next$, $frontv$ )
		if ($frontv$ &amp; $plus2$ != "G")
			$metaph$ = "%%$metaph$J"
		else
			$metaph$ = "%%$metaph$K"
		endif
	else
		$R2$ = $cnt$ - 3
		activate "METAPHONE".noghf( $workstring$[$R2$:1], $noghf$ )
		$R2$ = $cnt$ - 4
		if ($next$ = "H" &amp; !$noghf$ &amp; $workstring$[$R2$:1] != "H")
			$metaph$ = "%%$metaph$F"

		endif
	endif
endif

				if ($nptr$ = "H")
					activate "METAPHONE".varson( $previous$, $varson$ )
					activate "METAPHONE".vowel( $previous$, $vowel$ )
					$R2$ = $vowel$
					activate "METAPHONE".vowel( $next$, $vowel$ )
					if (!($varson$ | $R2$) &amp; $vowel$ )
						$metaph$ = "%%$metaph$H"
					endif
				endif 

				if ($nptr$ = "K")
				   if ($previous$ != "C" ) 
					$metaph$ = "%%$metaph$K"
				   endif
				endif

				if ($nptr$ = "P")
					if ($next$ = "H")
						$metaph$ = "%%$metaph$F"
						$CNT$ = $CNT$ + 1
					else
						$metaph$ = "%%$metaph$P"
					endif
				endif

				if ($nptr$ = "Q") 
					$metaph$ = "%%$metaph$K"
				endif

				if ($nptr$ = "S")
					$R2$ = ($plus2$ = "O" | $plus2$ = "A")
					if ($next$ = "H" | ($next$ = "I" &amp; $R2$))

						$metaph$ = "%%$metaph$X"
					else
						$metaph$ = "%%$metaph$S"
					endif	
					;$cnt$ = $cnt$ + 1
				endif

				if ($nptr$ = "T")
				  $R2$ = ($plus2$ = "O" | $plus2$ = "A")
				  if ($next$ = "I" &amp; $R2$)
				     $metaph$ = "%%$metaph$X"
				  else
				     if ($next$ = "H")
				       $metaph$ = "%%$metaph$O"
				     else
				       if ($next$ != "C" | $plus2$ != "H")
					  $metaph$ = "%%$metaph$T"
				       endif
				     endif	
				  endif
				endif

				if ($nptr$ = "V") 
					$metaph$ = "%%$metaph$F"
				endif

				if ($nptr$ = "W" | $nptr$ = "Y")
					
					activate "METAPHONE".vowel( $next$, $vowel$ )
					if ($vowel$) 
						$metaph$ = "%%$metaph$%%$nptr$"
					endif
				endif

				if ($nptr$ = "X")
					if ($cnt$ = 1)
						$metaph$ = "%%$metaph$S"
					else
						$metaph$ = "%%$metaph$K"
						$KSflag$ = 1 
						; Insert 'S' on next pass
					endif
				endif

				if ($nptr$ = "Z")
					$metaph$ = "%%$metaph$S"
				endif
			endif
		endif
duploop:
		$cnt$ = $cnt$ + 1
	endwhile
alldone:
	output_string = ""
	output_string = "%%$metaph$"
	return (1)
done

operation upcases
params
	string input_string 	: IN
	string Poutput_string 	: INOUT
endparams

; Uppercase with strip &amp; duplicate removal
	$letter$ = "ABCDEFGHIJKLMNOPQRSTUVWXYZ"
	$numbr$  = "abcdefghijklmnopqrstuvwxyz"

	Poutput_string = ""
	length input_string
	$length$ = $result
	$cnt$ = 1
	repeat
	   scan $numbr$,input_string[$cnt$:1]
	   if ($result &gt; 0)
		Poutput_string = "%%Poutput_string%%$letter$[$result:1]"
	   else
		scan $letter$,input_string[$cnt$:1]
		if ($result &gt; 0)
		  Poutput_string = "%%Poutput_string%%input_string[$cnt$:1]"
		endif
	   endif
	   $cnt$ = $cnt$ + 1
	until ($cnt$ &gt; $length$)
	$cnt$ = 1
	$R0$ = 2
	$r1$ = ""
	repeat
		$R0$ = $cnt$ + 1
		if (Poutput_string[$R0$:1] != Poutput_string[$cnt$:1] | 		    Poutput_string[$R0$:1] = "C")
			$r1$ = "%%$r1$%%Poutput_string[$cnt$:1]"
		endif
		$cnt$ = $cnt$ + 1
	until ($cnt$ &gt; $length$)
	Poutput_string = $r1$
	return (1)
done

operation same
params
	string input_string	: IN
	boolean same		: OUT
endparams

	same = "F"
	if (input_string != "")
		scan $vsvcn$,input_string
		if ($result != 1)
			$R3$ = ($result * 2) - 1
		else
			$R3$ = $result
		endif
		activate "METAPHONE".BINARY( $number($vsvfn$[$r3$:2]), $r2$)
		if ($R2$[2:1] = "1") same = "T"
	endif
	return (1)
done

operation  varson
params	
